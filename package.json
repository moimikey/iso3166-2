{
  "name": "iso3166-2",
  "version": "0.1.0",
  "description": "Tiny, fast, modular ISO 3166-2 lookup library",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/iso3166-2.git"
  },
  "keywords": [
    "iso3166",
    "iso3166-2",
    "iso-3166-2",
    "country",
    "subdivisons",
    "counties",
    "territories",
    "countries",
    "iso",
    "provinces",
    "divisions"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://michael.hertzberg.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/iso3166-2/issues"
  },
  "homepage": "https://github.com/moimikey/iso3166-2",
  "devDependencies": {
    "babel": "~6.23.0",
    "babel-runtime": "~6.26.0",
    "babelify": "~7.3.0",
    "brfs": "~1.4.1",
    "csv2": "~0.1.1",
    "event-stream": "~3.3.2",
    "file-size": "~1.0.0",
    "hyperquest": "~2.1.2",
    "jsesc": "~2.5.1",
    "jsonstream2": "~1.1.0",
    "lodash": "~4.17.4",
    "punycode": "~2.1.0",
    "request": "~2.81.0",
    "stripquotes": "~0.2.3",
    "tap-spec": "~4.1.0",
    "tape": "~4.8.0",
    "through2-reduce": "~1.1.1",
    "uglifyjs": "~2.4.10",
    "zlib-browserify": "~0.0.3"
  },
  "browserify": {
    "transform": [
      "babelify",
      "brfs"
    ]
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "prepublish": "npm run generate && npm run build && npm run test",
    "build": "browserify src/index.js --standalone iso31662 --outfile lib/index.js",
    "generate": "babel-node bin/generate",
    "test": "tape tests/index.js | tap-spec",
    "benchmark": "matcha -R plain"
  }
}
